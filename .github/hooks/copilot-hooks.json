{
  "hooks": {
    "SessionStart": [
      {
        "type": "command",
        "command": "python .github/hooks/session_start.py",
        "windows": "python .github\\hooks\\session_start.py",
        "timeout": 15
      }
    ],
    "SubagentStart": [
      {
        "type": "command",
        "command": "python .github/hooks/subagent_start.py",
        "windows": "python .github\\hooks\\subagent_start.py",
        "timeout": 15
      }
    ],
    "PreCompact": [
      {
        "type": "command",
        "command": "python .github/hooks/pre_compact.py",
        "windows": "python .github\\hooks\\pre_compact.py",
        "timeout": 15
      }
    ]
  }
}